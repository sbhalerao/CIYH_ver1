
<div class="three-col">
<% @catwalks.each do|catwalk| %>
<br>
<% if catwalk.id >= 17 %>
<%= image_tag catwalk.catpic.url, :height => '300'%>

<br>

<p> Posted by <%= (catwalk.user).name %> </p>
<%= link_to 'vote for this post!', voteup_path(:id => catwalk.id), :remote => true  %>
<div id="votecount<%=catwalk.id %>"> <%=catwalk.votes_for %>  </div>
<% end %>
<% end %>

</div>