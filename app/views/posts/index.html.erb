<h1>Listing posts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Name</th>
    <th>Content</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @posts.each do |post| %>
  <tr>
    <td><%= post.title %></td>
    <td><%= post.name %></td>
    <td><%= post.content %></td>
	<td><%= post %></td>
	<td><%= post.id %></td>
	<td> <%= post.cat1x %> </td>
    <td><%= link_to 'Show', post %></td>
    <td><%= link_to 'Edit', edit_post_path(post) %></td>
    <td><%= button_to 'Destroy', post, :confirm => 'Are you sure?', :method => :delete %></td>
   
  </tr>
<% end %>
</table>

